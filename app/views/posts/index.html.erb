<p style="color: green"><%= notice %></p>

<% content_for :title, "Posts" %>

<h1>投稿一覧</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <%= link_to post, class: "post-link" do %>
      <%= render post %>
    <% end %>
  <% end %>
</div>

<%= link_to "新規投稿", new_post_path , class: "link new-post" %>
<br>
<%= link_to "アカウントトップ", dashboard_index_path , class: "link ac_top" %>
